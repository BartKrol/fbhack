{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "2.1",
  "description": "Gets information from Google.",
  "icons": {"256": "icon.png"},
  "background": {
    "persistent": false,
    "scripts": [
      "bower_components/jquery/dist/jquery.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [

        "content.js"
      ]
    }
  ],
  "permissions": [
    "http://*.google.com/",
    "https://*.google.com/",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "browser_action": {
    "default_title": "Make this page red"
  },
  "web_accessible_resources": [
    "bower_components/jquery/dist/jquery.min.js",
    "bower_components/jquery/dist/jquery.min.map",
    "custom_js.js"
  ]
}
